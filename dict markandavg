students = [
    {"name": "Alice", "marks": [85, 90, 78,80,70]},
    {"name": "Bob", "marks": [70, 88, 92,100,60]},
    {"name": "Charlie", "marks": [95, 85, 85,80,60]}
]


for student in students:
    name = student["name"]
    marks = student["marks"]
    average = sum(marks) / len(marks)
    print(f"Name: {name}, Average Marks: {average}")
